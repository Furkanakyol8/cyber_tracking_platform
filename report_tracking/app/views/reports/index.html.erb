<div class="w-100">
  <table class="table m-5 text-justify: inter-character">
    <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
      <th scope="col">  </th>
    </tr>
    </thead>
    <tbody>
    

    <% @reports.first(10).each do |report| %>
      <tr>
        <th><%= report.id %></th>
        <td><%= report.title %></td>
        <td class="col-2"><%= report.description %></td>
        <td><button type="button" class="btn btn-light"> <%= link_to "Show this report", report %></button></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<%= link_to "New report", new_report_path %>

  <td><button type="button" class="btn btn-light"><%= link_to "New Report", new_report_path%></button></td>

</div>


 